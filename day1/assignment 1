public class finalamount{

     public static void main(String []args){
     double price1= 15.5;
     double price2= 4.5;
     int price3= 30;
     double tax=5;
     double sum=price1+price2+price3;
     double taxamount=sum*(tax)/100;
     double finalamount=taxamount+(sum); 
     System.out.println(finalamount);  
     }
}
